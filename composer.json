{
    "name": "sixteenhands/filament-dynamic-filter",
    "description": "Dynamic table filters for Filament with caching, indicators and panel access control",
    "keywords": [
        "sixteenhands",
        "laravel",
        "filament",
        "filament-dynamic-filter",
        "filter",
        "tables"
    ],
    "homepage": "https://github.com/16hands/filament-dynamic-filter",
    "support": {
        "issues": "https://github.com/16hands/filament-dynamic-filter/issues",
        "source": "https://github.com/16hands/filament-dynamic-filter"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "16Hands",
            "email": "carl@16hands.co.nz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/tables": "^4.0|^5.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "SixteenHands\\FilamentDynamicFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SixteenHands\\FilamentDynamicFilter\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SixteenHands\\FilamentDynamicFilter\\FilamentDynamicFilterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}